{
  "name": "virtualized-material-graph-table",
  "version": "1.0.01",
  "description": "virtualized material table whith filters, sort and pieGraph for columns",
  "private": false,
  "homepage": "http://andre166.github.io/virtualized-material-graph-table",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "babel": {
    "presets": [
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-modules-commonjs",
        {
          "allowTopLevelThis": true
        }
      ]
    ]
  },
  "peerDependencies": {
    "react": "16.4.1",
    "prop-types": "^15.7.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "3.4.0",
    "@material-ui/icons": "3.0.2",
    "@material-ui/pickers": "3.0.0",
    "date-fns": "^2.16.0",
    "highcharts": "^7.1.1",
    "highcharts-react-official": "^2.1.3",
    "lodash": "^4.17.21",
    "material-ui-pickers": "^2.0.0",
    "moment": "^2.26.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "^6.0.1",
    "react-scripts": "^3.4.3",
    "react-virtualized": "9.21.1",
    "@date-io/moment": "^0.0.2",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-modules-commonjs": "^7.13.8",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.3.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.3.0",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "react": "^16.13.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "publish:npm": "babel src/components -d dist --copy-files",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Andr√© Mesquita",
  "license": "MIT"
}
